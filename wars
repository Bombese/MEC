-- [ TAB: EVENT ]
-------------------------------------------------------------------------------
local EventTab = Window:AddTab({ Title = "Event", Icon = "calendar" })

EventTab:AddSection("Event Teleport (วาร์ปกิจกรรม)")

--// 1. วาร์ปไปยึดจุดจับกุม (ตามพิกัดที่ระบุ)
EventTab:AddButton({
    Title = "Teleport to Capture Point (วาร์ปไปจุดยึด)",
    Description = "TP to Capture Point (X:-502, Y:177, Z:-1031)",
    Callback = function()
        local char = lp.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if hrp then
            hrp.CFrame = CFrame.new(-502.4, 177.0, -1031.0)
            Fluent:Notify({
                Title = "Event System",
                Content = "Arrived at Capture Point! (ถึงจุดยึดแล้ว)",
                Duration = 2
            })
        end
    end
})

--// 2. วาร์ปไปหา Oil (ระบบ Scan หาอัตโนมัติ)
EventTab:AddButton({
    Title = "Teleport to Oil (วาร์ปไปหา Oil)",
    Description = "Scan and TP to Oil location in Map",
    Callback = function()
        local char = lp.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        local found = false

        -- ค้นหา Oil ใน Workspace (ตรวจสอบชื่อที่น่าจะเป็นไปได้)
        for _, v in pairs(workspace:GetDescendants()) do
            if v.Name == "Oil" or v.Name:find("Oil") then
                local targetPos = nil
                if v:IsA("Model") then
                    targetPos = v:GetModelCFrame()
                elseif v:IsA("BasePart") then
                    targetPos = v.CFrame
                end

                if targetPos and hrp then
                    hrp.CFrame = targetPos
                    found = true
                    Fluent:Notify({
                        Title = "Event System",
                        Content = "Oil Found! (เจอ Oil แล้ว)",
                        Duration = 2
                    })
                    break
                end
            end
        end

        if not found then
            Fluent:Notify({
                Title = "Error",
                Content = "Oil not found in this map! (ไม่พบ Oil ในแมพนี้)",
                Duration = 2
            })
        end
    end
})

EventTab:AddSection("Information")
EventTab:AddParagraph({
    Title = "How to use",
    Content = "ปุ่ม Oil จะทำการสแกนหา Object ที่ชื่อว่า Oil ในแมพแล้ววาร์ปไปหาทันที ส่วนจุดยึดจะวาร์ปไปตามพิกัดคงที่"
})

-------------------------------------------------------------------------------
-- [ TAB: TELEPORT ] - ระบบวาร์ปหาผู้เล่น
-------------------------------------------------------------------------------
local TPTab = Window:AddTab({ Title = "Teleport", Icon = "user" })

TPTab:AddSection("Player Teleport (วาร์ปหาผู้เล่น)")

local SelectedPlayer = nil
local FlyingToPlayer = false

--// 1. Dropdown เลือกรายชื่อผู้เล่น
local PlayerDropdown = TPTab:AddDropdown("PlayerDropdown", {
    Title = "Select Player (เลือกผู้เล่น)",
    Values = {}, -- จะอัปเดตอัตโนมัติ
    Multi = false,
    Default = nil,
    Callback = function(Value)
        SelectedPlayer = Value
    end
})

-- ฟังก์ชันอัปเดตรายชื่อคนในเซิร์ฟ
local function UpdatePlayerList()
    local pList = {}
    for _, p in pairs(Players:GetPlayers()) do
        if p ~= lp then
            table.insert(pList, p.Name)
        end
    end
    PlayerDropdown:SetValues(pList)
end

-- อัปเดตรายชื่อตอนเปิด และตอนคนเข้า/ออก
UpdatePlayerList()
Players.PlayerAdded:Connect(UpdatePlayerList)
Players.PlayerRemoving:Connect(UpdatePlayerList)

--// 2. ปุ่มวาร์ป (Instant TP)
TPTab:AddButton({
    Title = "Teleport to Player (วาร์ปไปหา)",
    Description = "วาร์ปไปที่ตำแหน่งผู้เล่นทันที",
    Callback = function()
        if SelectedPlayer then
            local target = Players:FindFirstChild(SelectedPlayer)
            if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                lp.Character.HumanoidRootPart.CFrame = target.Character.HumanoidRootPart.CFrame * CFrame.new(0, 3, 0)
                Fluent:Notify({ Title = "TP Success", Content = "Teleported to " .. SelectedPlayer, Duration = 3 })
            end
        else
            Fluent:Notify({ Title = "Error", Content = "Please select a player first!", Duration = 3 })
        end
    end
})

--// 3. ปุ่มบินไปหา (Fly to Player / Tween)
TPTab:AddToggle("FlyToToggle", {
    Title = "Auto Fly to Player (บินไปหา/ติดตาม)",
    Default = false,
    Callback = function(Value)
        FlyingToPlayer = Value
    end
})

-- Logic สำหรับการบินติดตาม
RunService.Heartbeat:Connect(function()
    if FlyingToPlayer and SelectedPlayer then
        pcall(function()
            local target = Players:FindFirstChild(SelectedPlayer)
            local myHRP = lp.Character and lp.Character:FindFirstChild("HumanoidRootPart")
            local targetHRP = target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")
            
            if myHRP and targetHRP then
                -- บินไปข้างบนหัวเป้าหมายเล็กน้อย (กันติดพื้น)
                myHRP.CFrame = myHRP.CFrame:Lerp(targetHRP.CFrame * CFrame.new(0, 5, 2), 0.1)
                myHRP.Velocity = Vector3.new(0, 0, 0) -- หยุดแรงโน้มถ่วง
            end
        end)
    end
end)

TPTab:AddSection("Refresh List")
TPTab:AddButton({
    Title = "Refresh Player List",
    Description = "กดหากรายชื่อไม่อัปเดต",
    Callback = function()
        UpdatePlayerList()
    end
})

-------------------------------------------------------------------------------
-- [ TAB: EXECUTOR ] - ปรับปรุงใหม่
-------------------------------------------------------------------------------
local ExecTab = Window:AddTab({ Title = "Executor", Icon = "terminal" })

ExecTab:AddSection("Executor Information (ข้อมูลตัวรัน)")

-- ป้องกัน Error กรณี identifyexecutor คืนค่าไม่ครบ
local success, name, version = pcall(function()
    local n, v = identifyexecutor()
    return n or "Unknown", v or "N/A"
end)

-- ถ้าเรียกฟังก์ชันไม่ได้เลย ให้ใส่ค่า Default
if not success then
    name, version = "Unknown", "N/A"
end

ExecTab:AddParagraph({
    Title = "Current Executor",
    Content = "Name: " .. tostring(name) .. "\nVersion: " .. tostring(version)
})

ExecTab:AddSection("Check Capabilities")

ExecTab:AddButton({
    Title = "Check Internal UI Version",
    Description = "ตรวจสอบเวอร์ชันของ Fluent UI ที่ใช้อยู่",
    Callback = function()
        Fluent:Notify({
            Title = "UI Info",
            Content = "Fluent UI Version: 1.1.0",
            Duration = 3
        })
    end
})

ExecTab:AddButton({
    Title = "Check Script Support",
    Description = "เช็คว่าตัวรันของคุณรองรับฟังก์ชันสำคัญไหม",
    Callback = function()
        local support = ""
        support = support .. "FireTouch: " .. (firetouchinterest and "✅" or "❌") .. "\n"
        support = support .. "GetHui: " .. (gethui and "✅" or "❌") .. "\n"
        support = support .. "ProtectGui: " .. ((syn and syn.protect_gui) and "✅" or "❌")
        
        Fluent:Notify({
            Title = "Compatibility Check",
            Content = support,
            Duration = 5
        })
    end
})

