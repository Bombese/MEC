-------------------------------------------------------------------------------

-- บรรทัดนี้ต้องอยู่ล่างสุดของ Script ทั้งหมดเสมอ!
Window:SelectTab(1)

Fluent:Notify({
    Title = "Felon HUB",
    Content = "Script Loaded Successfully!",
    Duration = 5
})
