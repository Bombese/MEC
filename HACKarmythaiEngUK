		},
		["‚öîÔ∏è ‡∏Å‡∏≤‡∏£‡πÇ‡∏à‡∏°‡∏ï‡∏µ"] = {
			["‚öîÔ∏è kill bot(Risk)"] = "https://pastebin.com/raw/DczvQZyU",
			["üéØ Aim bot"] = "https://gist.githubusercontent.com/Aimboter477387/582af6aec49782899d5d375ab239039e/raw"
		},
		["üîß -HACK"] = {
			["üéÅ Conjure items"] = "https://raw.githubusercontent.com/yofriendfromschool1/Sky-Hub-Backup/main/gametoolgiver.lua",
			["üî• FPS"] = "https://raw.githubusercontent.com/XNEOFF/FPS-BOOSTER/main/FPSBooster.txt",
			["üé© Hitbox 32%"] = "https://pastebin.com/raw/iRyTtfmf",
			["üí™üèª god mod"] = "https://pastebin.com/raw/EjrTrMwC"
		},
		["üòà people "] = {
			["üåå backhoe"] = "https://pastebin.com/raw/pkZnU5P5",
			["ü§ß kick"] = "https://pastebin.com/raw/TXMNj1yy"
		},
		["üîç ESP & ESP Bot"] = {
			["üîç ESP Players"] = "https://pastebin.com/raw/ZkLhNuDL",
			["ü§ñ ESP BOT,NPC(beta)"] = "https://pastebin.com/raw/q26QuUBF"
		}
	}

	-- üß† ‡πÇ‡∏´‡∏•‡∏î‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô
	local function clearFunctions()
		for _, c in pairs(funcFrame:GetChildren()) do
			if c:IsA("TextButton") then
				c:Destroy()
			end
		end
	end

	local function loadFunctions(category)
		clearFunctions()
		local y = 8
		for name, url in pairs(functionCategories[category]) do
			local btn = Instance.new("TextButton")
			btn.Parent = funcFrame
			btn.Size = UDim2.new(1, -16, 0, 30)
			btn.Position = UDim2.new(0, 8, 0, y)
			btn.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
			btn.Text = name
			btn.TextColor3 = Color3.fromRGB(240, 240, 240)
			btn.Font = Enum.Font.GothamBold
			btn.TextSize = 13
			Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 6)

			btn.MouseEnter:Connect(function()
				btn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
			end)
			btn.MouseLeave:Connect(function()
				btn.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
			end)
			btn.MouseButton1Click:Connect(function()
				loadstring(game:HttpGet(url))()
			end)
			y += 38
		end
		funcFrame.CanvasSize = UDim2.new(0, 0, 0, y)
	end

	-- ‡∏õ‡∏∏‡πà‡∏°‡∏´‡∏°‡∏ß‡∏î
	local y = 8
	for category, _ in pairs(functionCategories) do
		local catBtn = Instance.new("TextButton")
		catBtn.Parent = categoryFrame
		catBtn.Size = UDim2.new(1, -16, 0, 35)
		catBtn.Position = UDim2.new(0, 8, 0, y)
		catBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
		catBtn.Text = category
		catBtn.Font = Enum.Font.GothamBold
		catBtn.TextSize = 14
		catBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
		Instance.new("UICorner", catBtn).CornerRadius = UDim.new(0, 6)

		catBtn.MouseButton1Click:Connect(function()
			loadFunctions(category)
		end)

		y += 42
	end

	-- üîò Toggle
	local toggle = Instance.new("TextButton")
	toggle.Parent = ScreenGui
	toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	toggle.Size = UDim2.new(0, 45, 0, 45)
	toggle.Position = UDim2.new(0, 50, 0, 50)
	toggle.Font = Enum.Font.GothamBold
	toggle.Text = "HACK"
	toggle.TextSize = 18
	toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
	Instance.new("UICorner", toggle).CornerRadius = UDim.new(1, 0)

	local isVisible = true
	toggle.MouseButton1Click:Connect(function()
		isVisible = not isVisible
		main.Visible = isVisible
	end)
end

createUI()